<div id="search" >

		<%= form_for @search do |f| %>

		 <p>
			<%= f.label :nome_contains, "Descrição:" %><br />
			<%= f.text_field :nome_contains, :class=>"clearme", :placeholder=>"Executa pesquisa no campo descrição" %>
		 </p>


		<%= f.submit  "Pesquisar",:class=>"g-button" %>
		<%=f.submit  "Limpar", :type=>"reset", :class=>"reset_search g-button", :id=>"reset_search"  %> <br /> 

		<% end %>
	
</div>
