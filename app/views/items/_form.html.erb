<%= form_for(@item) do |f| %>

	<%= render 'shared/error_messages', :object => f.object %>

  <div class="field">
    <%= f.label :produto_id %><br />
    <%= f.text_field :produto_id %>
  </div>
  <div class="field">
    <%= f.label :marca_id %><br />
    <%= f.text_field :marca_id %>
  </div>
  <div class="field">
    <%= f.label :parecer_id %><br />
    <%= f.text_field :parecer_id %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
